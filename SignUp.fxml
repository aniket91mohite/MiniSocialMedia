<GridPane fx:id="signUpWindow" alignment="CENTER" hgap="10.0" style="-fx-background-color: #212121;" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainWindow.SignUpController">
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Create An Account" textAlignment="CENTER" textFill="WHITE" GridPane.columnSpan="2147483647" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="first_name" promptText="Enter First Name" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="last_name" promptText="Enter Last Name" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="email_id" promptText="Enter Email ID" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <TextField fx:id="contact_no" promptText="Enter Contact Number" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <PasswordField fx:id="password" prefHeight="25.0" prefWidth="196.0" promptText="Enter Password" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <PasswordField fx:id="cpassword" promptText="Confirm Password" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      <TextField fx:id="birthday" promptText="Enter Birth Date" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="9" />
      <HBox alignment="CENTER" prefHeight="25.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="10">
         <children>
            <RadioButton fx:id="gender" mnemonicParsing="false" text="Male" textAlignment="CENTER" textFill="WHITE">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <toggleGroup>
                  <ToggleGroup fx:id="toggleGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="Female" textFill="WHITE" toggleGroup="$toggleGroup" />
         </children>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </HBox>
      <TextField fx:id="about" promptText="About Your Self" style="-fx-background-radius: 15;" GridPane.columnIndex="1" GridPane.rowIndex="11" />
      <Button mnemonicParsing="false" onAction="#saveData" prefHeight="25.0" prefWidth="93.0" style="-fx-background-color: cyan; -fx-background-radius: 15;" text="Finish" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="12" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#rollBackScene" prefHeight="25.0" prefWidth="94.0" style="-fx-background-color: cyan; -fx-background-radius: 15;" text="Cancel" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="13" />
